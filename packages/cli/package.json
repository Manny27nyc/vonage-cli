{
    "name": "kja-cli",
    "description": "CLI to interact with Vonage APIs",
    "version": "1.0.0-alpha.19",
    "author": "Vonage Dev Rel <devrel@vonage.com>",
    "bin": {
        "vonage": "./bin/run"
    },
    "bugs": "https://github.com/Vonage/vonage-cli/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.0",
        "@oclif/plugin-plugins": "^1.9.1",
        "@types/fs-extra": "^5.0.2",
        "fs-extra": "^9.0.1",
        "kja-cli-plugin-applications": "^1.0.0-alpha.19",
        "kja-cli-plugin-numbers": "^1.0.0-alpha.19",
        "kja-cli-utils": "^1.0.0-alpha.19",
        "tslib": "^2.1.0"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/node_modules",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/Vonage/vonage-cli",
    "keywords": [
        "oclif"
    ],
    "license": "Apache 2.0",
    "main": "dist/index.js",
    "oclif": {
        "commands": "./src/commands",
        "bin": "vonage",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins",
            "kja-cli-plugin-applications",
            "kja-cli-plugin-numbers"
        ],
        "hooks": {
            "init": "./dist/hooks/init"
        },
        "topics": {
            "auth": {
                "hidden": true
            },
            "app": {
                "hidden": true
            },
            "account": {
                "hidden": true
            },
            "number": {
                "hidden": true
            },
            "secrets": {
                "hidden": true
            },
            "plugins": {
                "hidden": true
            }
        }
    },
    "repository": "Vonage/vonage-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "../../node_modules/.bin/jest test",
        "test-watch": "../../node_modules/.bin/jest test --watch",
        "version": "oclif-dev readme && git add README.md",
        "build": "rm -rf ./dist && tsc -b"
    },
    "gitHead": "91a480e3c9effde9b43be756e107b64c2ab07431"
}
