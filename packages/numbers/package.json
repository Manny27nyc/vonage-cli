{
  "name": "@vonage/cli-plugin-numbers",
  "version": "0.0.0",
  "author": "Luke Oliff @lukeocodes",
  "bugs": "https://github.com/Vonage/vonage-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@vonage/cli-utils": "^0.0.0",
    "tslib": "^1.14.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Vonage/vonage-cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Vonage/vonage-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "../../node_modules/.bin/jest test",
    "test-watch": "../../node_modules/.bin/jest test --watch",
    "version": "oclif-dev readme && git add README.md"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1"
  }
}
