{
    "name": "@vonage/cli-plugin-applications",
    "description": "A plugin for Vonage CLI",
    "version": "1.0.0-beta.5",
    "author": "Vonage Dev Rel <devrel@vonage.com>",
    "bugs": "https://github.com/Vonage/vonage-cli/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/parser": "^3.8.5",
        "@types/lodash.merge": "^4.6.6",
        "@vonage/cli-utils": "^1.0.0-beta.5",
        "chalk": "^2.4.2",
        "lodash": "^4.17.20",
        "prompts": "^2.4.0",
        "tslib": "^2.1.0",
        "typescript": "^4.2.2",
        "unique-names-generator": "^4.3.1"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/dist",
        "/bin",
        "/npm-shrinkwrap.json",
        "/node_modules",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/Vonage/vonage-cli",
    "keywords": [
        "oclif-plugin"
    ],
    "license": "Apache 2.0",
    "oclif": {
        "commands": "./dist/commands",
        "bin": "oclif-example",
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "Vonage/vonage-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "../../node_modules/.bin/jest test",
        "test-watch": "../../node_modules/.bin/jest test --watch",
        "version": "oclif-dev readme && git add README.md",
        "build": "rm -rf ./dist && tsc -b"
    },
    "gitHead": "e04bdc633e101ef1c1fe0190ed5d2f8452253dc4"
}
